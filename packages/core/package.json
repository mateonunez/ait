{
  "name": "@ait/core",
  "version": "0.1.0",
  "description": "Core building blocks for AIt: errors, result, http, shared types",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "prepare": "pnpm generate:openapi && pnpm build",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf dist coverage",
    "lint": "biome format && biome lint && biome check",
    "lint:fix": "biome format --write && biome lint --write --unsafe && biome check --write",
    "generate:openapi": "tsx ./src/openapi/openapi.generator.ts"
  },
  "keywords": ["core", "errors", "types", "http"],
  "author": "@mateonunez <mateonunez95@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "crypto-js": "^4.2.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@redocly/openapi-core": "^2.6.0",
    "@types/crypto-js": "^4.2.2",
    "openapi-typescript": "^7.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
