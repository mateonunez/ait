{
  "name": "@ait/scheduler",
  "version": "1.0.0",
  "description": "AIt's schedluer",
  "main": "dist/scheduler.entrypoint.js",
  "scripts": {
    "dev": "node --require @swc-node/register --watch src/scheduler.entrypoint.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "prepare": "pnpm build",
    "clean": "rm -rf dist coverage",
    "start": "node ./dist/scheduler.entrypoint.js",
    "start:manual": "node --require @swc-node/register src/scheduler.entrypoint.ts --manual",
    "clean:state": "node --require @swc-node/register src/scripts/clean-state.ts",
    "lint": "biome format && biome lint && biome check",
    "lint:fix": "biome format --write && biome lint --write --unsafe && biome check --write",
    "pretest": "test -f .env.test || cp .env.example .env.test",
    "test": "glob src/**/*.test.ts -c 'c8 node --require @swc-node/register --require ./test-setup.ts --test'",
    "test:watch": "glob src/**/*.test.ts -c 'c8 node --require @swc-node/register --require ./test-setup.ts --test --watch'"
  },
  "keywords": [],
  "author": "@mateonunez <mateonunez95@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ait/connectors": "workspace:*",
    "@ait/core": "workspace:*",
    "@ait/postgres": "workspace:*",
    "@ait/qdrant": "workspace:*",
    "@ait/redis": "workspace:*",
    "@ait/retove": "workspace:*",
    "bullmq": "^5.66.2",
    "dotenv": "^17.2.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.7",
    "@types/dotenv": "^8.2.3",
    "typescript": "^5.9.3"
  }
}
