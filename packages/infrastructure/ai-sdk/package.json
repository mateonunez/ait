{
  "name": "@ait/ai-sdk",
  "version": "1.0.0",
  "description": "AIt's AI SDK integration with Ollama, RAG, and tool calling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "prepare": "pnpm build",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf dist coverage",
    "lint": "biome format && biome lint && biome check",
    "lint:fix": "biome format --write && biome lint --write --unsafe && biome check --write",
    "pretest": "test -f .env.test || cp .env.example .env.test",
    "test": "glob src/**/*.test.ts -c 'c8 tsx --require ./test-setup.ts --test'",
    "test:e2e": "tsx --require ./test-setup.ts src/services/text-generation/text-generation.service.e2e.ts --concurrency=1"
  },
  "keywords": ["ai-sdk", "ollama", "rag", "embeddings", "llm"],
  "author": "@mateonunez <mateonunez95@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ait/core": "workspace:*",
    "@ait/qdrant": "workspace:*",
    "@ait/store": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "ai": "^6.0.3",
    "chrono-node": "^2.9.0",
    "dotenv": "^17.2.3",
    "js-tiktoken": "^1.0.21",
    "langfuse": "^3.38.6",
    "ai-sdk-ollama": "^3.0.0",
    "stopword": "^3.1.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/sinon": "^17.0.4",
    "@types/stopword": "^2.0.3",
    "c8": "^10.1.3",
    "sinon": "^21.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
