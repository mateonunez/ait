{
  "name": "@ait/ai-sdk",
  "version": "1.0.0",
  "description": "AIt's AI SDK integration with Ollama, RAG, and tool calling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "clean": "rm -rf dist coverage",
    "lint": "biome format && biome lint",
    "lint:fix": "biome format --write && biome lint --write --unsafe",
    "pretest": "test -f .env.test || cp .env.example .env.test",
    "test": "glob src/**/*.test.ts -c 'c8 tsx --require ./test-setup.ts --test'",
    "test:e2e": "tsx --require ./test-setup.ts src/services/text-generation/text-generation.service.e2e.ts --concurrency=1"
  },
  "keywords": ["ai-sdk", "ollama", "rag", "embeddings", "llm"],
  "author": "@mateonunez <mateonunez95@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ait/qdrant": "workspace:*",
    "ai": "^5.0.82",
    "cohere-ai": "^7.19.0",
    "dotenv": "^17.2.3",
    "langfuse": "^3.30.3",
    "ollama-ai-provider-v2": "^1.5.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ait/connectors": "workspace:*",
    "@types/node": "^24.9.1",
    "@types/sinon": "^17.0.4",
    "c8": "^10.1.3",
    "sinon": "^21.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
