{
  "name": "@ait/connectors",
  "version": "1.0.0",
  "description": "AIt's connectors",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json --watch",
    "build": "tsc -p ./tsconfig.build.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf dist coverage",
    "lint": "biome format && biome lint && biome check",
    "lint:fix": "biome format --write && biome lint --write --unsafe && biome check --write",
    "pretest": "test -f .env.test || cp .env.example .env.test",
    "test": "glob ./**/*.test.ts -c 'c8 tsx --require ./test-setup.ts --test --test-concurrency=1'",
    "test:watch": "glob ./**/*.test.ts -c 'c8 tsx --require ./test-setup.ts --test --watch'",
    "prepare": "pnpm build"
  },
  "keywords": [],
  "author": "@mateonunez <mateonunez95@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ait/ai-sdk": "workspace:*",
    "@ait/core": "workspace:*",
    "@ait/postgres": "workspace:*",
    "@ait/qdrant": "workspace:*",
    "@ait/redis": "workspace:*",
    "@fastify/secure-session": "^8.2.0",
    "@octokit/plugin-paginate-rest": "^13.2.0",
    "@octokit/rest": "^22.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "fastify-cors": "^6.1.0",
    "fastify-env": "^2.2.0",
    "postgres": "^3.4.7",
    "undici": "^7.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^16.1.0",
    "@octokit/types": "^15.0.0",
    "@redocly/openapi-core": "^2.6.0",
    "@types/sinon": "^17.0.4",
    "openapi-typescript": "^7.10.1",
    "pino-pretty": "^13.1.2",
    "sinon": "^21.0.0",
    "typescript": "^5.9.3"
  }
}
